# Database
DATABASE_URL="mysql://user:password@localhost:3306/bolt_ai_salon"

# Server
PORT=3000
NODE_ENV=development
APP_NAME="Bolt AI Salon"

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# Logging
LOG_LEVEL=debug

# Stripe
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_MONTHLY_PRICE_ID=price_monthly_id
STRIPE_YEARLY_PRICE_ID=price_yearly_id

# Twilio
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# AI Providers
# OpenAI - for LLM/NLU capabilities
OPENAI_API_KEY=sk-your_openai_api_key

# Vapi - for call orchestration
VAPI_API_KEY=your_vapi_api_key
VAPI_ASSISTANT_ID=your_vapi_assistant_id

# ElevenLabs - for TTS (optional, can use OpenAI TTS instead)
ELEVENLABS_API_KEY=your_elevenlabs_api_key
